{
  "name": "nuxt-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:nuxt": "nuxt",
    "dev:api": "aspida --watch",
    "dev:path": "pathpida --watch",
    "dev:mock": "aspida-mock --watch",
    "dev:pages-css": "tcm pages --watch",
    "dev:components-css": "tcm components --watch",
    "build": "run-p build:* && nuxt build",
    "generate": "run-p build:* && nuxt generate",
    "build:api": "aspida",
    "build:path": "pathpida",
    "build:mock": "aspida-mock",
    "build:pages-css": "tcm pages",
    "build:components-css": "tcm components",
    "lint": "yarn lint:js && yarn lint:prettier && yarn lint:style",
    "lint:js": "eslint --ext .ts,.tsx,.js --ignore-path .gitignore .",
    "lint:prettier": "prettier --ignore-path .gitignore --check \"./**/*.{ts,tsx,js}[!.d.ts]\"",
    "lint:style": "stylelint \"**/*.css\" --ignore-path .gitignore",
    "lint:fix": "npm run lint:js -- --fix && prettier --ignore-path .gitignore --write \"./**/*.{ts,tsx,js}[!.d.ts]\" &&  npm run lint:style -- --fix",
    "typecheck": "run-p build:* && tsc --noEmit"
  },
  "dependencies": {
    "@aspida/fetch": "^1.7.1",
    "@nuxtjs/composition-api": "^0.26.0",
    "core-js": "^3.16.2",
    "nuxt": "^2.15.8"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.0",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "aspida-mock": "^0.8.2",
    "csstype": "^3.0.8",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "nuxt-vite": "^0.1.3",
    "pathpida": "^0.16.0",
    "prettier": "^2.3.2",
    "prettier-plugin-organize-imports": "^2.3.3",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.5.0",
    "stylelint-config-standard": "^22.0.0",
    "typed-css-modules": "^0.7.0"
  }
}
